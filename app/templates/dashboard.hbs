<div class="container-fluid">
  <div class="row">
    <div class="col-sm-3 col-md-2 sidebar">
      <ul class="nav nav-sidebar">
        <li class="active"><a href="#">Holdings</a></li>
        <li><a href="#">News</a></li>
        <li><a href="#">Analytics</a></li>
      </ul>
      <ul class="nav nav-sidebar">
        <li><a href="">Nav item</a></li>
        <li><a href="">Nav item again</a></li>
        <li><a href="">One more nav</a></li>
        <li><a href="">Another nav item</a></li>
        <li><a href="">More navigation</a></li>
      </ul>
      <ul class="nav nav-sidebar">
        <li><a href="">Nav item again</a></li>
        <li><a href="">One more nav</a></li>
        <li><a href="">Another nav item</a></li>
      </ul>
    </div>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
      <h1 class="page-header">Dashboard</h1>

      <div class="row placeholder">
        <div class="col-md-4 placeholder">
          <img data-src="holder.js/200x200/auto/sky" class="img-responsive" alt="Generic placeholder thumbnail">
          <h4>Week Earnings</h4>
          <span class="text-muted">These are the your earnings graphed over the week.</span>
        </div>
        <div class="col-md-4 placeholder">
          <img data-src="holder.js/200x200/auto/vine" class="img-responsive" alt="Generic placeholder thumbnail">
          <h4>Quarter Earnings</h4>
          <span class="text-muted">Your earnings graphed over the quarter.</span>
        </div>
        <div class="col-md-4 placeholder">
          <img data-src="holder.js/200x200/auto/sky" class="img-responsive" alt="Generic placeholder thumbnail">
          <h4>Year Earnings</h4>
          <span class="text-muted">Earnings over the year.</span>
        </div>
      </div>

      <h2 class="sub-header">Your holdings</h2>
      <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Symbol</th>
              <th>#</th>
              <th>Cost at Purchase</th>
              <th>Current Cost</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            {{#each}}
              <tr>
                <td>{{symbol}}</td>
                <td>{{number}}</td>
                <td>{{cost}}</td>
                <td>{{worth}}</td>
                <td>
                  <button class="btn btn-sm btn-success" {{action 'view'}}>view</button>
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

